{
  "name": "gametimer-admin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "altstart": "node -e \"require('./src/app').handler(null, null)\"",
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "lint": "eslint .",
    "seq-create": "npm run seq-migrate && npm run seq-seed",
    "seq-undo": "sequelize db:migrate:undo",
    "seq-drop": "sequelize db:migrate:undo:all",
    "seq-migrate": "sequelize db:migrate",
    "seq-seed": "sequelize db:seed:all",
    "seq-newseed": "sequelize seed:generate"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^15.0.0",
    "express": "~4.16.1",
    "express-jwt": "^6.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "@babel/node": "^7.16.8",
    "jest": "^27.4.7"
  }
}
